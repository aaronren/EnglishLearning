/* miniprogram/pages/dailyWords.wxss */

page {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
}

.scroll-view {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
}

/* 底部按钮 & scroll view底部的留白 */

.bottom-button {
  width: 100%;
  bottom: 0;
  position: fixed;
}

.bottom-space {
	bottom: 0;
	min-height: 120rpx;
}

/* cu-load完成时的文案，ui库中只有over文案 */

.cu-load.done::before {
	content: "\e64a";
}

.cu-load.done::after {
	content: "开始学习吧";
}
